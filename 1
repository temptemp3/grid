bash() { /opt/homebrew/bin/bash "${@}" ; }
list-assets () 
{ 
    coinboy portfolio:assets | tr ',' '\t'
}
_cleanup_desktop_recordings() {
  local ext="mov"
  test -d "Recordings" || mkdir -pv "${_}"
  find . -mindepth 1 -maxdepth 1 -type f -name \*Screen\ Recording\*.${ext} -print0 \
  | xargs -0 -I {} mv -v "{}" "${_}/"
}
_cleanup_desktop_screenshots() {
  local ext="png"
  test -d "Screenshots" || mkdir -pv "${_}"
  find . -mindepth 1 -maxdepth 1 -type f -name \*Screen\ Shot\*.${ext} -print0 \
  | xargs -0 -I {} mv -v "{}" "${_}/"
}
_cleanup_desktop_links() {
  local ext="webloc"
  test -d "Links" || mkdir -pv "${_}"
  find . -mindepth 1 -maxdepth 1 -type f -name \*.${ext} -print0 \
  | xargs -0 -I {} mv -v "{}" "${_}/"
}
_cleanup_desktop() {
  ${FUNCNAME}_screenshots
  ${FUNCNAME}_recordings
  ${FUNCNAME}_links
}
_cleanup() {
  ${FUNCNAME}_desktop
}
PATH="${PATH}:~/.yarn/bin"
_ () {
	helm -n launcher upgrade --install -f values-dev.yaml contract-launcher charts/contract-launcher
}
_sum() {
  gawk 'BEGIN{SUM=0}{SUM+=$(1)}END{print SUM}' -
}
_ipfsls2gawk () {
	ipfs ls "${1:-cid}" | gawk '{print "s/\""$(3)"\"/\"ipfs:\\/\\/"$(1)"\"/g"}'
}
