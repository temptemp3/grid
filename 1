_mt_login_testnet_kyc_yes() {
 curl -X 'PATCH' \
  "https://api.testnet.motleyestates.com/admin/users/${1}" \
  -H 'accept: application/json' \
  -H 'Authorization: Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1c2VyX2lkIjozLCJlbWFpbCI6Im5pY2thdDl0QGdtYWlsLmNvbSIsImV4cCI6MTY3NjUyMzQ5Nn0.hLA-lF_c226mkhPylQEWUpEJ9snmR0WwUxN__vEGkyE' \
  -H 'Content-Type: application/json' \
  -d '{
  "has_kyc": "true"
}'
}
